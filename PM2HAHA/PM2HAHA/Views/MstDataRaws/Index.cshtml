@model IEnumerable<PM2HAHA.Models.MstDataRaw>

@{
    ViewData["Title"] = "Index";
}

<h2>Data AQI Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Aqi)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Idx)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.City)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Longitude)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Latitude)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Co)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.H)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.No2)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.O3)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.P)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Pm10)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Pm25)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.So2)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.T)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.W)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.time)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CreateAt)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UploadAt)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Aqi)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Idx)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.City)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Longitude)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Latitude)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Co)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.H)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.No2)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.O3)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.P)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Pm10)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Pm25)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.So2)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.T)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.W)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.time)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.CreateAt)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.UploadAt)
                </td>
                <td>
                    <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                    <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                    <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
                </td>
            </tr>
            
        }
        
    </tbody>
</table>
<div class="form-group">
    <div class="col-md-offset-2 col-md-10">

        <button type="button" onclick="location.href='@Url.Action("Index","MstRsis")'">Graph</button>
    </div>
</div>